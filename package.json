  "dependencies": {
    "bootstrap": "^4.1.1",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.3"
  },
  "devDependencies": {
    "concurrently": "^3.6.0",
    "cors-anywhere": "^0.4.1",
